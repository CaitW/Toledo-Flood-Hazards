<!-- <li class="adv-only"> -->
	<div class="panel panel-default">
		<div class="panel-heading"  role="tab"  id = 'attributeHeading'>
			<a data-toggle="collapse" href="#attributeBody" aria-expanded="false" aria-controls="collapseOne"data-parent="#accordion_Data">
				<h4 class="panel-title">Unit of Measurement <i class="fa fa-chevron-right pull-right"></i></h4>


				<p class="panel-subtitle text-right">
					<%-(_DOM.fieldSelector=='BldgDmgPct') ? 'Percent Damage' :  'Monetary Losses'%>
				</p>
			</a>
		</div>
		<!-- /.panel-heading -->
		<div id="attributeBody" class="panel-collapse collapse" role="tabpanel" aria-labelledby="attributeHeading">
				<div class="list-group">
					<% appdata.floodAttributes.forEach(function(item) {%>
						<div class="radio list-group-item <% if (item.value==_DOM.fieldSelector){%> active<%}%>">
							<label role="button">
								<span class="radio-icon"><i class='fa fa-check'style="margin-left: -15px"></i></span>
								<input type="radio" name="fieldRadios" value="<%= item.value %>"
								<% if (item.value==_DOM.fieldSelector){%> checked<%}%> >
								<%= item.text %>
							</label>
						</div>
					<% }) %>
				</div>
			<!-- </div> -->
			<!-- /.panel-body -->
		</div>
		<!-- /#attributeBody -->
	</div>
	<!-- /.panel -->
<!-- </li> -->

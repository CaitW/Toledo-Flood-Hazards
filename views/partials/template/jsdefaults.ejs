<script>
		var appData = {};
		// Assign data to default globals
		appData.content = <%- JSON.stringify(content) %>
		// add hazard data 
		appData.hazardData = {};
		appData.hazardData.allYear= <%- JSON.stringify(allYearData) %>
		appData.hazardData.byID=<%- JSON.stringify(databyID) %>
		appData.hazardData.byYear=<%- JSON.stringify(dataByYear) %>
		appData.hazardData.fullList =<%- JSON.stringify(fullList) %>
		// add layer data
		appData.layerData = {};
		appData.layerData.stormwaterJSON=<%- JSON.stringify(stormwaterJSON) %>
		appData.layerData.chesterCreekWatershed=<%- JSON.stringify(chesterCreekWatershed) %>
		appData.layerData.futureLandUse =<%- JSON.stringify(futureLandUse) %>
		// add information about default selections
		appData.defaults = defaults;
		// file location (see domSettings.js)
		loc = <%- JSON.stringify(location) %>

		var delay = (function(){
		  var timer = 0;
		  return function(callback, ms){
		    clearTimeout (timer);
		    timer = setTimeout(callback, ms);
		  };
		})();

	// Sidebar Drag Action
	$("#menu-toggle").click(function(e) {
	    e.preventDefault();
	    $("#wrapper,.nav.right-side,#menu-toggle").toggleClass("toggled");

	    delay(function(){
	    	chartResize()
	    	map.invalidateSize()
	    },1000)

	});
</script>

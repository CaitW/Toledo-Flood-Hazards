<script>
		// Assign data to default globals
		var appdata= <%- JSON.stringify(appdata) %>
			allYearData= <%- JSON.stringify(allYearData) %>
			databyID=<%- JSON.stringify(databyID) %>
			dataByYear=<%- JSON.stringify(dataByYear) %>
			fullListJSON=<%- JSON.stringify(fullList) %>
			fullList = fullListJSON.mylist

			silverCreekWatershed=<%- JSON.stringify(silverCreekWatershed) %>
			futureLandUse =<%- JSON.stringify(futureLandUse) %>
			currentLandUse=<%- JSON.stringify(currentLandUse) %>


			startingValues = <%- JSON.stringify(_DOM) %>
			serverVariables = <%- JSON.stringify(variables) %>
			loc = <%- JSON.stringify(location) %>


		/* Globals */
		var nE=	startingValues.nE,
			sW=	startingValues.sW,
			mode= serverVariables.mode,
			damagesCurrent= serverVariables.damagesCurrent,
			damagesCompare= serverVariables.damagesCompare,
			showCompareFeatures=startingValues.showCompareFeatures,
			depthGridCurrent= serverVariables.depthGridCurrent,
			currentAttribute=startingValues.fieldSelector,
			damageIndexWidth= serverVariables.damageIndexWidth,
			minimumDamageIndex= serverVariables.minimumDamageIndex,
			maximumDamageIndex= serverVariables.maximumDamageIndex,
			compareType = startingValues.compareType


		var delay = (function(){
		  var timer = 0;
		  return function(callback, ms){
		    clearTimeout (timer);
		    timer = setTimeout(callback, ms);
		  };
		})();

	// Sidebar Drag Action
	$("#menu-toggle").click(function(e) {
	    e.preventDefault();
	    $("#wrapper,.nav.right-side,#menu-toggle").toggleClass("toggled");

	    delay(function(){
	    	chartResize()
	    	map.invalidateSize()
	    },1000)

	});
</script>

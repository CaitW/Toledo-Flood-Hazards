
	<div class="panel panel-default">
		<div class="panel-heading"  role="tab"  id = 'dataHeading'>
			<a data-toggle="collapse" href="#dataBody" aria-expanded="false" aria-controls="collapseOne" data-parent="#accordion_Data">
				<h4 class="panel-title">Climate / Land Use Scenario<i class="fa fa-chevron-right pull-right"></i></h4>
			<p class="panel-subtitle text-right">
				<%-appdata.scenarios[_DOM.scenarioSelector].text %>
			</p>
			</a>
		</div>
		<!-- /.panel-heading -->
		<div id="dataBody" class="panel-collapse collapse" role="tabpanel" aria-labelledby="dataHeading">
			<div id="scenario-list" class="list-group numbered-radio">
				<% for(var i=0; i<appdata.scenarios.length; i++) {%>
					<div class="radio list-group-item <% if(i==_DOM.scenarioSelector){%>active <%}%>">
						<label>
							<span class="radio-icon"><i class='fa fa-check'></i></span>
							<span class="radio-icon"><i class='fa fa-exchange'></i></span>
							<input type="radio" name="scenarioRadios" data-udf="<%= appdata.scenarios[i].udf %>" data-compare='<%= appdata.scenarios[i].compare %>' data-dg="<%= appdata.scenarios[i].dg %>" data-compare-climate="<%= appdata.scenarios[i]['compare-climate'] %>" data-compare-landuse="<%= appdata.scenarios[i]['compare-landuse'] %>" data-compare-opposite="<%= appdata.scenarios[i]['compare-opposite'] %>" data-width="<%= appdata.scenarios[i].width %>" value="<%= appdata.scenarios[i].value%>"<% if(i==_DOM.scenarioSelector){%>checked <%}%> >
							<span class="numbered-choice"><%= appdata.scenarios[i].text %></span>
						</label>
					</div>
				<% } %>
			</div>
			<!-- /#scenario-list -->
		</div>
		<!-- /#dataBody -->
	</div>
	<!-- /.panel -->
